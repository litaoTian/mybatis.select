<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.mapper.IScore">

    <select id="selectScoreById" resultType="com.pojo.Score" parameterType="integer">
        select id,java,android from score where id=#{id};
    </select>
</mapper>